            name="severity"
            value="ignore" />

        <metadata
            name="net.sf.eclipsecs.core.lastEnabledSeverity"
            value="inherit" />
    </module>

    <module name="RegexpSingleline" >

        <property
            name="severity"
            value="ignore" />

        <property
            name="format"
            value="\s+$" />

        <property
            name="message"
            value="Line has trailing spaces." />

        <metadata
            name="net.sf.eclipsecs.core.lastEnabledSeverity"
            value="inherit" />
    </module>

    <module name="SuppressionFilter" >

        <property
            name="file"
            value="${project_loc}/android_checkstyle_suppression.xml" />
    </module>

    <module name="SuppressionCommentFilter" />

